<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.thesischecker.controller.AnalysisEntity" table="analysis" schema="" catalog="thesis">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="date">
            <column name="date" sql-type="datetime"/>
        </property>
        <property name="description">
            <column name="description" sql-type="longtext" length="2147483647"/>
        </property>
        <property name="result">
            <column name="result" sql-type="decimal" precision="18" scale="2"/>
        </property>
        <property name="fileType">
            <column name="file_type" sql-type="varchar"/>
        </property>
        <property name="filePath">
            <column name="file_path" sql-type="varchar"/>
        </property>
        <property name="plainText">
            <column name="plain_text" sql-type="longtext" length="2147483647"/>
        </property>
        <property name="userId">
            <column name="user_id" sql-type="int"/>
        </property>
        <property name="createdAt">
            <column name="created_at" sql-type="datetime" not-null="true"/>
        </property>
        <property name="updatedAt">
            <column name="updated_at" sql-type="datetime" not-null="true"/>
        </property>
        <property name="deletedAt">
            <column name="deleted_at" sql-type="datetime"/>
        </property>
        <set name="User" inverse="true">
            <key>
                <column name="id" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.thesischecker.controller.UserEntity"/>
        </set>
        <set name="Resources" table="analysis_has_resource" schema="" catalog="thesis">
            <key>
                <column name="analysis_id" not-null="true"/>
            </key>
            <many-to-many not-found="ignore" class="com.thesischecker.controller.ResourceEntity">
                <column name="resource_id" not-null="true"/>
            </many-to-many>
        </set>
    </class>
</hibernate-mapping>