<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.thesischecker.dto.UserUpdateEntity" table="user_update" schema="" catalog="thesis">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="token">
            <column name="token" sql-type="varchar"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar"/>
        </property>
        <many-to-one name="userEntity" class="com.thesischecker.dto.UserEntity">
            <column name="user_id" not-null="false"/>
        </many-to-one>
    </class>
</hibernate-mapping>