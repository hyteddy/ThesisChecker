<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class userId="com.thesischecker.dto.AnalysisHasResourceEntity" table="analysis_has_resource" schema=""
           catalog="thesis">
        <composite-id mapped="true" class="com.thesischecker.dto.AnalysisHasResourceEntityPK">
            <key-property userId="analysisId">
                <column userId="analysis_id" sql-type="int" not-null="true"/>
            </key-property>
            <key-property userId="resourceId">
                <column userId="resource_id" sql-type="int" not-null="true"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>